say { "mommaluck";
  say I can't STAND that woman!;
  load 24770;
  hold bottle-whiskey;
  toroom $n throws her head back.;
  drink whiskey;
  drink whiskey;
  drink whiskey;
  burp;
  hic;
  sigh;
  throw bottle floor;
}

say { "ogre";
  wink $N;
}

tell { "ogre";
  link say ogre;
}

tell { "mommaluck";
  link say mommaluck;
}
