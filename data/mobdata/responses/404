say { "Hello";
	cough;
	say Hello %n;
	}

give { "14423";
	checktoggle 7;
	smile %n;
 	say Bless you, this will make me better, I know it.;
	toroom $n quaffs a potion.;
	untoggle 7;
	toggle 8;
	say Yes, I feel much better already.;
	say Thank you.;
	}

