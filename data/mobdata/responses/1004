roomenter {"";
  random 10;
    randoption 1;
    randoption 2;
    randoption 3;
    randoption 4;
    randoption 5;
    randoption 6;
    randoption 7;
    randoption 8;
    randoption 9;
    randoption 10;
      say Hi there.;
  donerand;
}

package {"1";
  say Well, now that we are done with the formalities.;
  hit %n;
}

say {"hello";
  link package 1;
}

say {"hi";
  link package 1;
}

