roomenter {"";
            checktoggle 167;
            load 22426;
            toroom $n rolls a scroll up and slips it in his robes.;
            toroom $n suddenly notices you.;
            toroom $n looks at you with disgust.;
            say I don't think you should be here.;
            kill %n;
}

