Say { "gnara";
toroom $nhoots and hollers.;
	Say "Man, now that is a real peace of gnass.";
	Purr;
	Say "You should try to catch a peep sometime!";
	Nudge %n;
	}

Say { "gnelson";
	Say "I don't know what they were so upset about.";
	Shrug;
	Say "I just stopped in, it's not like I was doing anything..";
	Say "..oh no, wait.";
	Frown;
	Say "But she uses this ruler in class..";
	Stare;
	Say "..and I wanted to her teach me a lesson!";
	Spank %n;
	Grunt;
	Say "Oh, um, sorry.";
	}

Say { "gnicoli";
	Say "Man, that guy?";	
	Shake;
	Say "I think he had something for Ms. Gnelson..";
	Say "..and she was diggin' on me.";
	Grin;
	Say "So, you know how that goes.";
	Nod %n;
	}

Say { "gnomes";
	Say "Now, if there is anything right in The World, it's the gnome.";
	Say "Fine creatures those gnomes.  Hell, even the fellas are sexy.";
	toroom $nwipes the sweat off of his brow.;
	Say "Um, could you leave me now?  There is something, uh, that I need to do.";
	Peer;
	Twiddle;
	}

Say { "fiddling";
	Say "Hey, never you mind!";
	Say "Unless you're a gnome..";
	Peer %n;
	Say "..and want to give me a hand?";
	}

Say { "pervert";
	Slap %n;
	Say "Sorry!";
	Tell %n "Or, wait.  Did that turn you on?";
	Wink %n;
	}

Roomenter { "";
	Tell %n "Hey! I'm busy here!";
	Blush;
	} 
