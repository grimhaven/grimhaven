say {"hello";
	tovict $n looks you over.;
	tonotvict $n looks %n over.;
	say Hello, %n.;
	}

say {"bridge";
	say Okay, one bridge coming up... or down.;
	toroom $n chuckles at $s own joke.;
	pull lever;
	say It will only let one person across at a time.;
	}

say {"river";
	link say bridge;
	}

say { "other side";
	link say bridge;
	}

