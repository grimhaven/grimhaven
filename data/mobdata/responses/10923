roomenter {"";
            checktoggle 177;
            load 10945;
            toroom $n licks some blood from a boar's tusk.;
            grin %n;
            toroom $n slides the tusk into his belt.;
            say MMMM, I was wondering what to eat for dinner.;
            say Guess me be havin sum boil'd intruder.;
            untoggle 177;
            toggle 178;
            kill %n;
}

roomenter {"";
            checktoggle 178;
            kill %n;
}
