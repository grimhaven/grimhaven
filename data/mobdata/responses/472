roomenter { "466";
	grunt;
	tell %n Be good....or Crunch have to hurt.;
	tonotvict $n tells %n something;
	drool;
	}

say { "good";
	nod;
	say No fights...or Crunch break you in two.;
	snarl;
	}

	
